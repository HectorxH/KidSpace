{
    "settings": {
        "import/resolver": {
            "node": {
                "extensions": [".js",".ts",".d.ts"]
            }
        }
    },
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-console": "off",
        "import/extensions": "off"
    }
}
